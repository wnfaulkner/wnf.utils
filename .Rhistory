UniqueValsFromColnames <-
function(
df,
varnames
){
if(!any(varnames %in% names(df))){
stop(
paste0(
c(
"Varnames: '",
varnames,
"' do not exist in table names. Table names: '",
paste0(names(df), collapse = ", "),
"'."
),
collapse = ""
)
)
}
result <-
df[,names(df) %in% varnames] %>%
as.data.frame %>%
lapply(., unique) %>%
lapply(., RemoveNA) %>%
lapply(., as.character) %>%
lapply(., function(x) {strsplit(x, ",")}) %>%
lapply(., unlist) %>%
lapply(., unique)
return(result)
}
